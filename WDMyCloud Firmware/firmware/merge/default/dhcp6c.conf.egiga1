# Default dhpc6c configuration: it assumes the address is autoconfigured using
# router advertisements.

interface egiga1
{
#	information-only;

	request domain-name-servers;
	request domain-name;
#	request prefix-delegation;

	send ia-na 2;

	script "/etc/wide-dhcpv6/dhcp6c-script";
};

id-assoc na 2 {
	
};
